<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MenuController">
   <children>
      <Button fx:id="available" layoutX="193.0" layoutY="84.0" mnemonicParsing="false" onAction="#availableTrip" prefHeight="73.0" prefWidth="213.0" text="Check available trips">
         <tooltip>
            <Tooltip text="Click to check available trips" />
         </tooltip></Button>
      <Button fx:id="addTrip" layoutX="193.0" layoutY="188.0" mnemonicParsing="false" onAction="#addTrip" prefHeight="73.0" prefWidth="213.0" text="Add new trips">
         <tooltip>
            <Tooltip text="Click to add new trips" />
         </tooltip></Button>
      <Button fx:id="exit" cancelButton="true" layoutX="194.0" layoutY="296.0" mnemonicParsing="false" onAction="#exit" prefHeight="73.0" prefWidth="213.0" text="Exit">
         <tooltip>
            <Tooltip text="Click to exit from app" />
         </tooltip></Button>
      <Label layoutX="181.0" layoutY="31.0" prefHeight="45.0" prefWidth="237.0" text="Welcome to travel agency!">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
